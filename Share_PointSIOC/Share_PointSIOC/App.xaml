                  VerticalAlignment="{TemplateBinding VerticalAlignment}"
                  Width="{TemplateBinding Width}"
                  Height="{TemplateBinding Height}">
                                <Grid.RenderTransform>
                                    <CompositeTransform TranslateX="-1500" />
                                </Grid.RenderTransform>
                                <Grid x:Name="ContentRoot"
                    HorizontalAlignment="{TemplateBinding HorizontalAlignment}"
                    Height="{TemplateBinding Height}"
                    VerticalAlignment="{TemplateBinding VerticalAlignment}"
                    Width="{TemplateBinding Width}"
                    RenderTransformOrigin="0.5,0.5">
                                    <Border BorderThickness="{TemplateBinding BorderThickness}">
                                        <Border Margin="1"
                          Background="#FF282828">
                                            
                                            <Grid>
                                                <Grid.RowDefinitions>
                                                    <RowDefinition Height="Auto" />
                                                    <RowDefinition />
                                                </Grid.RowDefinitions>
                                                <Border BorderThickness="0,0,0,1" Width="Auto" Background="#FF282828">
                                                    
                                                    <Grid Height="Auto" Width="Auto">
                                                        <Grid.ColumnDefinitions>
                                                            <ColumnDefinition />
                                                            <ColumnDefinition Width="80" />
                                                        </Grid.ColumnDefinitions>
                                                        <ContentControl Content="{TemplateBinding Title}"
                                          HorizontalAlignment="Stretch"
                                          IsTabStop="False"
                                          Margin="24,12,6,10"
                                          VerticalAlignment="Center"
                                          Foreground="#FFBDBDBD" />
                                                        <Button x:Name="CloseButton"
                                  Grid.Column="1"
                                  HorizontalAlignment="Center"
                                  Height="50"
                                  IsTabStop="False"
                                  Style="{StaticResource MetroCrossButtonStyle}"
                                  VerticalAlignment="Center"
                                  Width="50"
                                  RenderTransformOrigin="0.5,0.5"
                                  Cursor="Hand">
                                                            <Button.RenderTransform>
                                                                <CompositeTransform ScaleX="0.6"
                                                  ScaleY="0.6" />
                                                            </Button.RenderTransform>
                                                        </Button>
                                                    </Grid>
                                                </Border>
                                                <Border Background="{TemplateBinding Background}" Margin="7" Grid.Row="1">
                                                    <ContentPresenter x:Name="ContentPresenter"
                                          ContentTemplate="{TemplateBinding ContentTemplate}"
                                          Content="{TemplateBinding Content}"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                          Margin="17,6,17,0" />
                                                </Border>
                                            </Grid>
                                        </Border>
                                    </Border>
                                </Grid>
                            </Grid>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
 <!-- //////////////////////////////////////////////////////////////// -->
        <Style x:Key="MetroCrossButtonStyle"
         TargetType="Button">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="Button">
                        <Grid Background="#FF282828">
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="FocusStates">
                                    <VisualState x:Name="Focused">
                                        <Storyboard>
                                            <DoubleAnimation Duration="0"
                                     To="1"
                                     Storyboard.TargetProperty="(UIElement.Opacity)"
                                     Storyboard.TargetName="FocusVisual"
                                     d:IsOptimized="True" />
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Unfocused" />
                                </VisualStateGroup>
                                <VisualStateGroup x:Name="CommonStates">
                                    <VisualStateGroup.Transitions>
                                        <VisualTransition GeneratedDuration="0:0:0.1" />
                                    </VisualStateGroup.Transitions>
                                    <VisualState x:Name="Normal" />
                                    <VisualState x:Name="MouseOver">
                                        <Storyboard>
                                            <ColorAnimation Duration="0"
                                    To="White"
                                    Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                    Storyboard.TargetName="border"
                                    d:IsOptimized="True" />
                                            <ColorAnimation Duration="0"
                                    To="White"
                                    Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                                    Storyboard.TargetName="path"
                                    d:IsOptimized="True" />
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Pressed">
                                        <Storyboard>
                                            <DoubleAnimation Duration="0"
                                     To="-28"
                                     Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.RotationX)"
                                     Storyboard.TargetName="border"
                                     d:IsOptimized="True" />
                                            <DoubleAnimation Duration="0"
                                     To="-14"
                                     Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.RotationY)"
                                     Storyboard.TargetName="border"
                                     d:IsOptimized="True" />
                                            <DoubleAnimation Duration="0"
                                     To="6"
                                     Storyboard.TargetProperty="(UIElement.Projection).(PlaneProjection.RotationZ)"
                                     Storyboard.TargetName="border"
                                     d:IsOptimized="True" />
                                            <ColorAnimation Duration="0"
                                    To="White"
                                    Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)"
                                    Storyboard.TargetName="FocusVisual"
                                    d:IsOptimized="True" />
                                            <ColorAnimation Duration="0"
                                    To="White"
                                    Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)"
                                    Storyboard.TargetName="FocusVisualCross"
                                    d:IsOptimized="True" />
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Disabled" />
                                </VisualStateGroup>
                            </VisualStateManager.VisualStateGroups>
                            <Border x:Name="border"
                    BorderBrush="#FFBDBDBD"
                    BorderThickness="2.5"
                    CornerRadius="90"
                    Margin="4">
                                <Border.Projection>
                                    <PlaneProjection />
                                </Border.Projection>
                                <Path x:Name="path"
                    Data="M10.999957,0 L14.999957,0 L14.999957,10.99999 L26,10.99999 L26,14.99999 L14.999957,14.99999 L14.999957,25.999998 L10.999957,25.999998 L10.999957,14.99999 L0,14.99999 L0,10.99999 L10.999957,10.99999 z"
                    Fill="#FFBDBDBD"
                    Margin="5"
                    RenderTransformOrigin="0.5,0.499999669881943"
                    Stretch="Fill"
                    UseLayoutRounding="False"
                    d:LayoutRounding="Auto"
                    Height="27">
                                    <Path.RenderTransform>
                                        <CompositeTransform Rotation="45"
                                      ScaleX="0.8"
                                      ScaleY="0.8" />
                                    </Path.RenderTransform>
                                </Path>
                            </Border>
                            <Border x:Name="FocusVisual"
                    BorderBrush="#FF96A7C8"
                    BorderThickness="2.5"
                    CornerRadius="90"
                    Margin="4"
                    Projection="{Binding Projection, ElementName=border}"
                    Opacity="0">
                                <Path x:Name="FocusVisualCross"
                    Data="M10.999957,0 L14.999957,0 L14.999957,10.99999 L26,10.99999 L26,14.99999 L14.999957,14.99999 L14.999957,25.999998 L10.999957,25.999998 L10.999957,14.99999 L0,14.99999 L0,10.99999 L10.999957,10.99999 z"
                    Fill="#FF96A7C8"
                    Margin="5"
                    RenderTransformOrigin="0.5,0.499999669881943"
                    Stretch="Fill"
                    UseLayoutRounding="False"
                    d:LayoutRounding="Auto">
                                    <Path.RenderTransform>
                                        <CompositeTransform Rotation="45"
                                      ScaleX="0.8"
                                      ScaleY="0.8" />
                                    </Path.RenderTransform>
                                </Path>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Application.Resources>
</Application>
